// ID : Title --> URL --> [Parent]
homepage                 : @(Homepage)               --> /
blogs                    : @(Blogs)                  --> /news/                      --> homepage
blogsdetail              : @(Blog detail)            --> /news/{0}/                  --> blogs
album                    : @(Album)                  --> /album/                     --> homepage
albumdetail              : @(Album detail)           --> /album/{0}/                 --> album
aboutus                  : @(About us)               --> /about_us/                  --> homepage
person                   : @(Album)                  --> /person/                    --> homepage
persondetail             : @(Album detail)           --> /person/{0}/                --> person
publication                    : @(Publication)                  --> /publication/                      --> homepage
publicationdetail              : @(Publication detail)            --> /publication/{0}/                  --> publication
